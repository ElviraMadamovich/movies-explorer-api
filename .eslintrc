{
  "env": {
    "es2021": true
  },
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "extends": [
    "airbnb-base",
    "prettier"
  ],
  "rules": {
    "no-console": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ],
    "comma-dangle": "off",
    "linebreak-style": "off",
  }
}